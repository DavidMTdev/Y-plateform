{{ dump(users) }}

<table class="table table-bordered">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Nom</th>
      <th scope="col">Email</th>
      <th scope="col">Age</th>
      <th scope="col">Date de cr√©ation</th>
      <th scope="col">Modifier / Supprimer</th>
    </tr>
  </thead>
  <tbody>
    {% for user in users %}
    <tr>
      <th scope="row">{{ user.id }}</th>
      <td>{{ user.username }} {{ user.lastname }}</td>
      <td>{{ user.mail }}</td>
      <td></td>
      <td></td>
      <td>logos</td>
    </tr>
    {% endfor %}
  </tbody>
</table>

{% endblock %}